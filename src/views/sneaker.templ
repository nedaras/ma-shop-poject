package views 

import (
	"nedas/shop/src/components"
	"nedas/shop/src/layouts"
)

// on ma phone onload does not work
templ Sneaker(product components.Product, sizes []string, loggedIn bool) {
	@layouts.Layout(product.Title) {
		@components.Header()
		<main>
			@components.Sneaker(product, sizes, loggedIn)
		</main>
	}
}
